-- Test basic arithmetic operations in JavaScript backend
-- This test validates that the JS backend correctly handles
-- integer and float arithmetic with proper operator precedence

main ||:
    -- Integer arithmetic
    x = 5 + 3
    y = 10 - 2
    z = 4 * 6
    w = 20 / 4
    
    -- Float arithmetic
    a = 3.14 + 2.86
